{
  "name": "@front-utils/react-hooks",
  "version": "0.3.0",
  "author": "IVGolubev",
  "description": "template",
  "repository": {
    "type": "git",
    "url": "https://github.com:garryk3/bun-pkg-template.git"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "browserslist": [
    "chrome > 83",
    "safari > 14.0"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "Javascript",
    "Utils",
    "React hooks"
  ],
  "license": "ISC",
  "type": "module",
  "types": "./types/src/index.d.ts",
  "publishConfig": {
    "access": "public",
    "ignore": [
      ".github/",
      "tsconfig.json"
    ]
  },
  "scripts": {
    "check-lint": "eslint --max-warnings=0 --fix",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "build": "rm -rf dist && rm -rf types && bun run build:types && bun run build:rspack",
    "build:rspack": "rspack build",
    "build:types": "tsc --p tsconfig.build.json",
    "pub": "bun run build && npm publish --access public"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --no-stash",
    "pre-push": "bun run test",
    "preserveUnused": true
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@front-utils/linter": "^0.9.6",
    "@rspack/cli": "1.3.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bun": "^1.1.13",
    "@types/eslint-plugin-jsx-a11y": "^6.9.0",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "bun": "^1.1.36",
    "eslint": "^9.13.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-compat": "^6.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": ">=6.0.0",
    "eslint-plugin-sonarjs": "^1.0.4",
    "globals": "^15.11.0",
    "happy-dom": "^15.11.6",
    "lint-staged": "^15.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "typescript-eslint": ">=8.0.0",
    "vitest": "^2.1.5"
  },
  "peerDependencies": {
    "react": "*",
    "@front-utils/utils": "^0.3.0"
  },
  "optionalDependencies": {
    "lodash.debounce": "^4.0.8"
  }
}